<div id="<%= dom_id article %>">
  <p>
    <strong>Num:</strong>
    <%= article.num %>
  </p>

  <p>
    <strong>Stock:</strong>
    <%= article.stock %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= article.price %>
  </p>

  <p>
    <strong>Model:</strong>
    <%= article.model_id %>
  </p>

  <p>
    <strong>Color:</strong>
    <%= article.color_id %>
  </p>

  <p>
    <strong>Cod:</strong>
    <%= article.cod %>
